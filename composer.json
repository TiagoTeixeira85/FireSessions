{
  "name": "kattsoftware/firesessions",
  "description": "PHP Sessions management library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "George Petculescu",
      "email": "opensource@kattsoftware.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "psr/log": "^1.0.2"
  },
  "autoload": {
    "psr-4": {
      "FireSessions\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "FireSessions\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.4 || ^7.0",
    "mikey179/vfsStream": "v1.6.5",
    "guzzlehttp/guzzle": "^6.3"
  }
}
